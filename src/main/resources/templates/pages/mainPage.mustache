                            <div class=""><i class="fa-solid fa-location-dot"></i>{{address}}</div>
                        </div>
                    </div>
                </div>
                {{/placesCafe}}

            </div>

        </div>


        <div id="tab-3" class="tab-content">

            <div class="tab-pane fade show active d-flex" style="justify-content: space-around;" id="hotel">

                {{#placesSpot}}
                <div class="col-md-3">
                    <div class="thumbnail">
                        <a href="/place/{{contentSeq}}">
                        </a>
                        <div class="caption">
                            <div class="m_font_lg m_font_bold m_ph_sm">{{title}}</div>
                            <div class=""><i class="fa-solid fa-location-dot"></i>{{address}}</div>
                        </div>
                    </div>
                </div>
                {{/placesSpot}}

            </div>

        </div>

        <div id="tab-3" class="tab-content">

            <div class="tab-pane fade show active d-flex" style="justify-content: space-around;" id="hotel">

                {{#placesActivity}}
                <div class="col-md-3">
                    <div class="thumbnail">
                        <a href="/place/{{contentSeq}}">
                        </a>
                        <div class="caption">
                            <div class="m_font_lg m_font_bold m_ph_sm">{{title}}</div>
                            <div class=""><i class="fa-solid fa-location-dot"></i>{{address}}</div>
                        </div>
                    </div>
                </div>
                {{/placesActivity}}


            </div>

        </div>
    </div>

</div>
</div>

