{{> /layout/header}}

<!-- 이미지 박스 시작 -->
<div class="container m_ph_md">
    <!-- Carousel https://getbootstrap.com/docs/5.1/components/carousel/ -->
    <div class="carousel-container justify-content-center">
        <!-- data-bs-interval: the amount of time to delay between automatically cycling an item. If false, carousel will not automatically cycle -->
        <!-- data-bs wrap: whether the carousel should cycle continuously (i.e go from first to last and vice-versa) -->
        <div id="vertical-carousel" class="carousel slide" data-bs-ride="carousel" data-bs-interval="4000" data-bs-wrap="true">
            <!-- Carousel slide indicators -->
            <ol class="carousel-indicators">
                <li data-bs-target="#vertical-carousel" data-bs-slide-to="0" class="active"></li>
                <li data-bs-target="#vertical-carousel" data-bs-slide-to="1"></li>
                <li data-bs-target="#vertical-carousel" data-bs-slide-to="2"></li>
                <li data-bs-target="#vertical-carousel" data-bs-slide-to="3"></li>
                <li data-bs-target="#vertical-carousel" data-bs-slide-to="4"></li>
            </ol>
            <div class="carousel-inner">
                <!-- Carousel item represents each slide -->
                <div class="carousel-item active" style="background-image:url(images/pethotel.jpg);">
                    <a href="javascript:linkListGo('숙박')">
                        <div class="container">
                            <div class="row" style="height: 100%">
                                <div class="display-3 text-center text-light m_txt_main m_mt_xxxl" style="font-size: 150px; opacity: 0.5;">
                                    숙박
                                </div>
                            </div>
                        </div>
                    </a>
                </div>
                <div class="carousel-item" style="background-image:url(images/petcafe.jpg);">
                    <a href="javascript:linkListGo('식음료')" onclick="ListGo('식음료')" >
                        <div class="container">
                            <div class="row align-items-center" style="height: 100%">
                                <div class="display-3 text-center text-light m_txt_main m_mt_xxxl" style="font-size: 150px; opacity: 0.5;">
                                    식음료
                                </div>
                            </div>
                        </div>
                    </a>
                </div>
                <div class="carousel-item" style="background-image:url(images/petplay.jpg);">
                    <a href="javascript:linkListGo('체험')" onclick="ListGo('체험')">
                        <div class="container">
                            <div class="row align-items-center" style="height: 100%">
                                <div class="display-3 text-center text-light m_txt_main m_mt_xxxl" style="font-size: 150px; opacity: 0.5;">
                                    체험
                                </div>
                            </div>
                        </div>
                    </a>
                </div>
                <div class="carousel-item" style="background-image:url(images/petoutdoor.jpg);">
                    <a href="javascript:linkListGo('여행지')" onclick="ListGo('여행지')">
                        <div class="container">
                            <div class="row align-items-center" style="height: 100%">
                                <div class="display-3 text-center text-light m_txt_main m_mt_xxxl" style="font-size: 150px; opacity: 0.5;">
                                    관광지
                                </div>
                            </div>
                        </div>
                    </a>
                </div>
                <div class="carousel-item" style="background-image:url(images/petclinic.jpg);">
                    <a href="javascript:linkListGo('동물병원')" onclick="ListGo('동물병원')">
                        <div class="container">
                            <div class="row align-items-center" style="height: 100%">
                                <div class="display-3 text-center text-light m_txt_main m_mt_xxxl" style="font-size: 150px; opacity: 0.5;">
                                    동물병원
                                </div>
                            </div>
                        </div>
                    </a>
                </div>
            </div>
        </div>

        <!-- Carousel buttons -->
        <button class="unclickable carousel-control-prev" type="button" data-bs-target="#vertical-carousel" data-bs-slide="prev">
            <!-- <span class="clickable carousel-control-prev-icon" aria-hidden="true"></span> -->
            <span class="visually-hidden">Previous</span>
        </button>
        <button class="unclickable carousel-control-next" type="button" data-bs-target="#vertical-carousel" data-bs-slide="next">
            <!-- <span class="clickable carousel-control-next-icon" aria-hidden="true"></span> -->
            <span class="visually-hidden">Next</span>
        </button>
    </div>
</div>

